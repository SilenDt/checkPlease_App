ignOut()\n    }\n\n    // auth.onAuthStateChanged((user) => {\n    //     console.log(\"this is the user \" + user)\n    // })\n\n    useEffect(() => {\n        const unsubscribe = auth.onAuthStateChanged(user => {\n            setCurrentUser(user)\n            setLoading(false)\n            setCurrentUser(user)\n        })\n        return unsubscribe\n    },[auth.onAuthStateChanged])\n\n    const value = {\n        currentUser,\n        SignIn,\n        SignUp,\n        logout,\n        resetPassword\n    }\n\n  return (\n    <AuthContext.Provider value={value}>\n        {!loading && children}\n    </AuthContext.Provider>\n  )\n}\n\n","/Users/fraserrussell/codeclan_work/code_clan_capstone_project_check_please/checkPlease_App/FrontEnd/src/components/auth/ForgotPassword.js",["70","71"],[],"import React, { useRef, useState } from \"react\";\nimport { Form, Card, Button, Alert, FormText } from \"react-bootstrap\"\nimport { Link } from \"react-router-dom\";\nimport { useAuth } from \"../../contexts/AuthContext\"\nimport SignUp from \"./SignUp\";\n\nconst ForgotPassword = () => {\n\n    const emailRef = useRef()\n    const [loading, setLoading] = useState(false)\n    const [message, setMessage] = useState()\n    const [error, setError] = useState()\n    const { resetPassword } = useAuth()\n\n    async function handleSubmit(e) {\n        e.preventDefault()\n        try {\n            setMessage(\"\")\n            setError(\"\")\n            setLoading(true)\n            await resetPassword(emailRef.current.value)\n            setMessage(\"Check you inbox for further instructions\")\n            // clear()\n        } catch {\n            setError(\"Failed to reset password\")\n\n        }\n        setLoading(false)\n    }\n\n    return (\n        <div>\n            <Card>\n                <Card.Body>\n                    <h2>Password Reset</h2>\n                    {error && <Alert variant=\"danger\">{error}</Alert>}\n                    {message && <Alert variant=\"success\">{message}</Alert>}\n                    <Form onSubmit={handleSubmit}>\n                        <Form.Group id=\"email\">\n                            <Form.Label>Email </Form.Label>\n                            <Form.Control type=\"email\" ref={emailRef} required></Form.Control>\n\n                        </Form.Group>\n                        <Button disabled={loading} className=\"w-100\" type=\"submit\">Reset Password</Button>\n                    </Form>\n                    <div className=\"w-100 text-center mt-3\">\n                        <Link to=\"/signin\">Sign In</Link>\n                    </div>\n\n                </Card.Body>\n            </Card>\n            <div className=\"w-100 text-center mt-3\">Don't have an account with us?\n                <Link to={\"/signup\"} className=\"w-100 text-center mt-3\"> Register</Link>\n            </div>\n        </div>\n    )\n}\n\nexport default ForgotPassword","/Users/fraserrussell/codeclan_work/code_clan_capstone_project_check_please/checkPlease_App/FrontEnd/src/components/auth/SignUp.js",[],[],"/Users/fraserrussell/codeclan_work/code_clan_capstone_project_check_please/checkPlease_App/FrontEnd/src/components/layout/NavBar.js",["72","73","74","75"],[],"/Users/fraserrussell/codeclan_work/code_clan_capstone_project_check_please/checkPlease_App/FrontEnd/src/components/auth/SignIn.js",["76"],[],"import React, {useRef, useState} from \"react\";\nimport {Form, Card, Button, Alert} from \"react-bootstrap\"\nimport {useAuth} from \"../../contexts/AuthContext\"\nimport {useNavigate, Link } from \"react-router-dom\";\nimport SignUp from \"./SignUp\";\n\nconst SignIn = () => {\n\n    const emailRef = useRef()\n    const passwordRef = useRef()\n    const [loading, setLoading] = useState(false)\n    const [error, setError] = useState()\n    const {SignIn} = useAuth()\n    const navigate = useNavigate()\n\n    async function handleSubmit(e) {\n        e.preventDefault()\n        try {\n            setError(\"\")\n       