bmit(e) {\n        e.preventDefault()\n        try {\n            setMessage(\"\")\n            setError(\"\")\n            setLoading(true)\n            await resetPassword(emailRef.current.value)\n            setMessage(\"Check you inbox for further instructions\")\n            // clear()\n        } catch {\n            setError(\"Failed to reset password\")\n\n        }\n        setLoading(false)\n    }\n\n    return (\n        <div>\n            <Card>\n                <Card.Body>\n                    <h2>Password Reset</h2>\n                    {error && <Alert variant=\"danger\">{error}</Alert>}\n                    {message && <Alert variant=\"success\">{message}</Alert>}\n                    <Form onSubmit={handleSubmit}>\n                        <Form.Group id=\"email\">\n                            <Form.Label>Email </Form.Label>\n                            <Form.Control type=\"email\" ref={emailRef} required></Form.Control>\n\n                        </Form.Group>\n                        <Button disabled={loading} className=\"w-100\" type=\"submit\">Reset Password</Button>\n                    </Form>\n                    <div className=\"w-100 text-center mt-3\">\n                        <Link to=\"/signin\">Sign In</Link>\n                    </div>\n\n                </Card.Body>\n            </Card>\n            <div className=\"w-100 text-center mt-3\">Don't have an account with us?\n                <Link to={\"/signup\"} className=\"w-100 text-center mt-3\"> Register</Link>\n            </div>\n        </div>\n    )\n}\n\nexport default ForgotPassword","/Users/victoriadorosenco/codeclan_work/CapstoneProject/checkPlease/checkPlease_App/FrontEnd/src/config/firebase.js",[],[],"/Users/victoriadorosenco/codeclan_work/CapstoneProject/checkPlease/checkPlease_App/FrontEnd/src/components/layout/SignedOutLinks.js",[],[],"/Users/victoriadorosenco/codeclan_work/CapstoneProject/checkPlease/checkPlease_App/FrontEnd/src/components/layout/SignedInLinks.js",["73"]